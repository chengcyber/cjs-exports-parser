{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "cjs",
    "exports"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chengcyber/cjs-exports-parser"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "cjs-exports-parser",
  "author": "Cheng Liu",
  "module": "dist/cjs-exports-parser.esm.js",
  "size-limit": [
    {
      "path": "dist/cjs-exports-parser.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cjs-exports-parser.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/fs-extra": "^9.0.4",
    "husky": "^4.3.0",
    "prettier": "^2.2.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@babel/core": "^7.12.9",
    "fs-extra": "^9.0.1"
  }
}
